vertexShaderPath: __axislines
#tessellationControlShaderPath: default_lit      #Optional
#tessellationEvaluationShaderPath: default_lit   #Optional
#geometryShaderPath: default_lit                 #Optional

options:
  cullMode: None
  faceDirection: Clockwise
  polygonMode: Fill
  enableRasterizerDiscard: false
  enableDepthClamping: false
  lineWidth: 1.0
  biasOptions:            #Optional
    constantFactor: 1.25
    clamp: 0.0
    slopeFactor: 1.75