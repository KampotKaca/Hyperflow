vertexShaderPath: default_unlit
#tessellationControlShaderPath: default_unlit      #Optional
#tessellationEvaluationShaderPath: default_unlit   #Optional
#geometryShaderPath: default_unlit                 #Optional

layout: default_unlit

options:
  cullMode: Back
  faceDirection: Clockwise
  polygonMode: Fill
  enableRasterizerDiscard: false
  enableDepthClamping: false
  lineWidth: 1.0
#  biasOptions: #Optional
#    constantFactor: 0.0
#    clamp: 0.0
#    slopeFactor: 0.0